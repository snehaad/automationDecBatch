package variables;

public class Local {

	public static void main(String[] args) {
		Local aa= new Local();
		aa.test1();
		aa.test2();
	}
		public void test1() {
			int a=100;
			String str="Tester";
			System.out.println(a);
			System.out.println(str);
		}
		public void test2() {
			int b=300;
			System.out.println(b);
		}

	}


